[
  {
    "key": "cmd+k n",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
      "snippet": "$CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND"
    }
  },
  {
    "key": "cmd+k d",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
      "snippet": "$CURRENT_DATE-$CURRENT_MONTH-$CURRENT_YEAR"
    }
  }
]
